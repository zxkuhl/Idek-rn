-- Fetch and run code from a URL (executor style)
local function runFromURL(url)
    local success, code = pcall(function()
        return game:HttpGet(url)
    end)
    if not success or not code then return end

    local func = loadstring(code)
    if func then
        pcall(func)
    end
end

-- Example: Execute TextBox code
I2L["16"].MouseButton1Click:Connect(function()
    local code = I2L["10"].Text -- TextBox input
    local func = loadstring(code)
    if func then
        pcall(func)
    end
end)

-- Example usage: runFromURL("https://raw.githubusercontent.com/YourUser/YourRepo/main/script.lua")
